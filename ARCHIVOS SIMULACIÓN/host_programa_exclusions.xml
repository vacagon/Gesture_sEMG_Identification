<?xml version="1.0" encoding="utf-8"?>
<rsccat locale="en_US" version="1.0">
   <SystemList>
      <System Name=".*">
         <ExclusionList>
            <Exclusion Rationale="Exclusion for this block only">
               <RuleList>
                  <Rule RegExp="off" SID="off" Type="Block">
                     <paramValue>Segmentation/Buffer</paramValue>
                  </Rule>
               </RuleList>
               <CheckIDList>
                  <CheckID>mathworks.codegen.SampleTimesTaskingMode</CheckID>
               </CheckIDList>
            </Exclusion>
         </ExclusionList>
      </System>
   </SystemList>
</rsccat>